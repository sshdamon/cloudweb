    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Click outside of the modal and close it
            window.onclick = function (e) {
                if ($(e.target).is('.modal')) {
                    closeModal($(e.target));
                }
            };
            // Use the escape key to close modal
            document.onkeyup = function (e) {
                e = e || window.event;
                if (e.keyCode == 27 && $('.modal--show').length === 1) {
                    closeModal($('.modal--show'));
                }
            };

            var analyticsData = {'Route': 'css-templates', 'Id': 532441, 'PageType': 'Block Detail Page', 'Lang': ''};

            var sendData = function(action, cb) {
                if (typeof fbq === 'function') {
                    fbq('track', 'Download');
                }

                var cbCounter = 0;

                function cbWithCounter() {
                    cbCounter++;
                    if (cbCounter === 2 && cb) {
                        cb();
                    }
                }

                var eventName = action !== 'edit-online' ? 'download' : 'edit-online';
                var eventCategory = action !== 'edit-online' ? 'download' : 'online';

                if (typeof sendAnalyticsData === 'function') {
                    sendAnalyticsData(eventName, analyticsData, cbWithCounter);
                }

                if (typeof gtag === 'function') {
                    gtag('event', action, { 'event_category': eventCategory, 'event_callback': cbWithCounter });
                } else {
                    cbCounter++;
                }
            }

            var sendAnalyticOrTimeout = decorateTimeout(sendData, 3000);

            $('.mac-download').click(function (e) {
                e.preventDefault();

                analyticsData.AppName = 'mac-app';
                if (false && typeof sendAmplitudeAnalyticsData === "function") {
                    sendAmplitudeAnalyticsData('Download', analyticsData);
                }

                showModal("sec-mac-download");
                showDownloadHint();

                sendAnalyticOrTimeout('mac-app', function () {
                    window.location = '/get/mac/';
                });
            });

            $('.win-download').click(function (e) {
                e.preventDefault();

                analyticsData.AppName = 'win-app';
                if (false && typeof sendAmplitudeAnalyticsData === "function") {
                    sendAmplitudeAnalyticsData('Download', analyticsData);
                }

                showModal("sec-win-download");
                showDownloadHint();

                sendAnalyticOrTimeout('win-app', function () {
                    window.location = '/get/windows/';
                });
            });

            $('.edit-online-btn, .edit-online-link').click(function (e) {
                e.preventDefault();

                analyticsData.AppName = 'online';
                if (false && typeof sendAmplitudeAnalyticsData === "function") {
                    sendAmplitudeAnalyticsData('Template Edit Online', analyticsData);
                }

                sendAnalyticOrTimeout('edit-online', function () {
                    window.location = '/Editor/Dashboard/EditOnline/532441';
                });
            });
        });
    </script>

